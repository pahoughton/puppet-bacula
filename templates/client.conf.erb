# Bacula Director Client Configuration for <%= @fdname %>
Client {
  Name = "<%= @fdname -%>-fd"
  Address = "<%= @fdname -%>"
  Catalog = "<%= @catalogname -%>"
  Password = "<%= @password -%>"
<% if @f_retention -%>
  File Retention = <%= @f_retention %>
<% end -%>
<% if @j_retention -%>
  Job Retention = <%= @j_retention %>
<% end -%>
<% if @auto_prune -%>
  AutoPrune = <%= @auto_prune %>
<% end -%>
<% if @max_jobs -%>
  Maximum Concurrent Jobs = <%= @max_jobs %>
<% end -%>
}
