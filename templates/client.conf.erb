# Bacula Director Client Configuration
#   for <%= @fd_host %>-fd
Client {
  Name = "<%= @fd_host %>-fd"
  FDAddress = "<%= @fd_host %>"
  Catalog = "<%= @hostname %>-cat"
  Password = "<%= @hostname %>-fd-pass"
  File Retention = <%= @f_retention %>
  Job Retention = <%= @j_retention %>
  AutoPrune = <%= @auto_prune %>
  Maximum Concurrent Jobs = <%= @max_jobs %>
  Priority = <%= @priority %>
}
